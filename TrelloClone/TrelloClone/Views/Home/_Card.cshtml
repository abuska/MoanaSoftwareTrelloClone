@using TrelloClone.Services;


<div style=" border-radius: 10px; padding: 1em; background-color: #ffffff; border-radius: 10px; margin: 5px;"
     data-id="'@Model.id'">
    <div class="card-selectable" onclick="prepareDetailForm('@Model.id');">
        <div style="font-size: 18px; font-weight: 500; text-transform: uppercase; color: #5e5e5e;">@Model.title</div>
        <div style="font-size: 14px; color: #969696;">@Model.description</div>
    </div>

</div>